# Note: Buildroot's and Meson's terminologies differ about the meaning
# of 'build', 'host' and 'target':
# - Buildroot's 'host' is Meson's 'build'
# - Buildroot's 'target' is Meson's 'host'

[binaries]
c = '/home/qemu-sgx/devel/optee/out-br/host/bin/arm-linux-gnueabihf-gcc'
cpp = '/home/qemu-sgx/devel/optee/out-br/host/bin/arm-linux-gnueabihf-g++'
ar = '/home/qemu-sgx/devel/optee/out-br/host/bin/arm-linux-gnueabihf-ar'
strip = '/home/qemu-sgx/devel/optee/out-br/host/bin/arm-linux-gnueabihf-strip'
pkgconfig = '/home/qemu-sgx/devel/optee/out-br/host/bin/pkgconf'
g-ir-compiler = '/home/qemu-sgx/devel/optee/out-br/host/arm-buildroot-linux-gnueabihf/sysroot/usr/bin/g-ir-compiler'
g-ir-scanner = '/home/qemu-sgx/devel/optee/out-br/host/arm-buildroot-linux-gnueabihf/sysroot/usr/bin/g-ir-scanner'

[built-in options]
c_args = ['-D_LARGEFILE_SOURCE', '-D_LARGEFILE64_SOURCE', '-D_FILE_OFFSET_BITS=64', '-Os', '-g0', '-D_FORTIFY_SOURCE=1'@PKG_TARGET_CFLAGS@]
c_link_args = [@PKG_TARGET_CFLAGS@]
cpp_args = ['-D_LARGEFILE_SOURCE', '-D_LARGEFILE64_SOURCE', '-D_FILE_OFFSET_BITS=64', '-Os', '-g0', '-D_FORTIFY_SOURCE=1'@PKG_TARGET_CFLAGS@]
cpp_link_args = [@PKG_TARGET_CFLAGS@]

[properties]
needs_exe_wrapper = true
sys_root = '/home/qemu-sgx/devel/optee/out-br/host/arm-buildroot-linux-gnueabihf/sysroot'
pkg_config_libdir = '/home/qemu-sgx/devel/optee/out-br/host/arm-buildroot-linux-gnueabihf/sysroot/usr/lib/pkgconfig:/home/qemu-sgx/devel/optee/out-br/host/arm-buildroot-linux-gnueabihf/sysroot/usr/share/pkgconfig'
pkg_config_static = 'false'

[host_machine]
system = 'linux'
cpu_family = 'arm'
cpu = 'cortex-a15'
endian = 'little'
